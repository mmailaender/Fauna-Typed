{
  "version": "0.2.7",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/mmailaender/Fauna-Typed.git"
  },
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src",
    "tsconfig.json"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "start": "tsdx watch",
    "build": "tsdx build",
    "generate:typedefs": "ts-node src/converter.ts schema.example.json",
    "test": "tsdx test --passWithNoTests",
    "lint": "pnpm eslint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "eslint": "eslint --ext src/**/*.ts src/**/*.tsx "
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "fauna-typed",
  "author": "Micha Mailaender",
  "module": "dist/fauna-typed.esm.js",
  "size-limit": [
    {
      "path": "dist/fauna-typed.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/fauna-typed.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "8.2.4",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@types/uuid": "9.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rollup-plugin-banner2": "1.2.2",
    "size-limit": "8.2.4",
    "tsdx": "0.14.1",
    "tslib": "2.5.0",
    "typescript": "5.0.2"
  },
  "dependencies": {
    "@types/node": "^20.4.6",
    "buffer": "6.0.3",
    "cosmiconfig": "8.1.3",
    "eslint": "6.8.0",
    "fauna": "^0.9.3",
    "husky": "8.0.3",
    "module": "1.2.5",
    "process": "0.11.10",
    "stream": "0.0.2",
    "ts-node": "10.9.1",
    "typescript-json-schema": "0.55.0",
    "usehooks-ts": "2.9.1",
    "uuid": "9.0.0",
    "zustand": "4.3.6"
  },
  "resolutions": {
    "typescript": "5.0.2"
  }
}
